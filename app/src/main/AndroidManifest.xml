            android:screenOrientation="portrait"
            android:theme="@style/RecipeTheme"
            android:label="Ajustes" >

        </activity>

        <activity
            android:name="geekgames.delichus4.seconds.ActivitySeguidos"
            android:parentActivityName="geekgames.delichus4.seconds.ActivityAjustes"
            android:configChanges="orientation"
            android:screenOrientation="portrait"
            android:theme="@style/RecipeTheme"
            android:label="Seguidos" >

        </activity>

        <activity
            android:name="geekgames.delichus4.seconds.RegistroUsuario"
            android:parentActivityName="geekgames.delichus4.Login"
            android:configChanges="orientation"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="stateHidden"
            android:theme="@style/RecipeTheme"
            android:label="Registrar" >

        </activity>

        <activity android:name="com.facebook.FacebookActivity"
            android:configChanges=
                "keyboard|keyboardHidden|screenLayout|screenSize|orientation"
            android:theme="@android:style/Theme.Translucent.NoTitleBar"
            android:label="@string/app_name" />

        <provider android:authorities="com.facebook.app.FacebookContentProvider807918469277605"
            android:name="com.facebook.FacebookContentProvider"
            android:exported="true"/>

    </application>



